<h1><%=@dojo.city %>'s' <%=@student.first_name%></h1>
<h4>Email: <%=@student.email%></h4>
<h3>Cohort</h3>
<table>
    <tr> 
        <th>First Name</th>
        <th>Last Name</th>
        <th>Email</th>
    </tr>
    <% @students.each do |student| %>
        <tr>
            <td><%= student.first_name%></td>
            <td><%= student.last_name%></td>
            <td><%= student.email%></td>
            <td><a href="">Show</a> | <a href="">Edit</a> | <a href="">Destroy</a></td>
        </tr>
    <% end %>
</table>
